                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_gravity="center"
                android:gravity="center"
                android:text="" />

            <TextView
                android:id="@+id/t_pengajuan"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="Pelayanan Pengajuan Akta Kelahiran"
                android:layout_marginStart="34dp"
                android:textColor="@color/black"
                android:textStyle="bold"
                />

            <!--text nomer KK-->
            <TextView
                android:id="@+id/t_pastikan"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_below="@+id/t_pengajuan"
                android:layout_alignStart="@+id/t_pengajuan"
                android:layout_marginStart="34dp"
                android:text="Pastikan Data Anda Sesuai Dan Sudah Terisi Semua"
                android:textColorHint="@color/grey" />

